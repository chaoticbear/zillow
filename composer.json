{
  "name": "chaoticbear/zillow",
  "type": "application",
  "description": "Laravel Wrapper for Zillow API",
  "keywords": [
    "laravel",
    "zillow",
    "php",
    "api"
  ],
  "homepage": "https://github.com/chaoticbear/zillow",
  "license": "MIT",
  "authors": [
    {
      "name": "Arjay Angeles",
      "email": "aqangeles@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/chaoticbear/zillow/issues"
  },
  "require": {
    "php": ">=5.4.0",
    "illuminate/support": "5.*",
    "guzzlehttp/guzzle": "4.*|5.*|6.*",
    "fabpot/goutte": "~2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0-dev"
    },
    "laravel": {
      "providers": [
        "Yajra\\Zillow\\ZillowServiceProvider"
      ],
      "aliases": {
        "Zillow": "Yajra\\Zillow\\Facades\\Zillow"
      }
    }
  },
  "autoload": {
    "psr-4": {
      "Chaoticbear\\Zillow\\": "src/"
    }
  },
  "minimum-stability": "stable"
}
